{
  "name": "cedwards-portfolio",
  "version": "0.0.1",
  "scripts": {
    "build": "webpack",
    "clean": "rm -rf dist",
    "lint": "yarn prettier && yarn eslint src",
    "prebuild": "yarn clean",
    "prettier": "prettier 'src/**/*.js' --write",
    "start": "yarn build && webpack-dev-server --hot",
    "test:watch": "karma start karma.conf.js",
    "test": "karma start karma.conf.js --single-run",
    "analyze": "ANALYZE_BUNDLE=1 yarn start"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "0.19.0",
    "classnames": "2.2.6",
    "p5": "^0.10.2",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-p5-wrapper": "^2.0.0",
    "react-router-dom": "5.1.2",
    "react-schema": "2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "chai": "4.2.0",
    "css-loader": "3.4.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-class-property": "1.1.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-mocha": "6.2.2",
    "eslint-plugin-react": "7.17.0",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-jasmine": "2.0.1",
    "karma-webpack": "4.0.2",
    "node-sass": "4.13.0",
    "prettier": "^1.19.1",
    "react-drill": "4.0.0-rc.1",
    "sass-loader": "8.0.0",
    "sinon": "8.0.2",
    "style-loader": "1.1.2",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  }
}
